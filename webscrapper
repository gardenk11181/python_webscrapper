import requests
from bs4 import BeautifulSoup

indeed_result = requests.get("https://www.indeed.com/jobs?q=python&limit=50")

indeed_soup = BeautifulSoup(indeed_result.text, "html.parser") # page의 모든 html을 가져온다.

pagination = indeed_soup.find("div", {"class":"pagination"}) # page number가 있는 block 가져옴

pages = pagination.find_all("a")  # "a"를 리스트로 가져옴
spans = []
for page in pages[0:-1]: # 마지막인 next제외하고 for loop 진행
    spans.append(int(page.find("span").string)) # 리스트에서 span 속 텍스트만 가져와서 리스트로 만듬

max_page = pages[-1]




